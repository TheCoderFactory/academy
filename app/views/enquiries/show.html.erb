<div class="container">
<div class="row">
  <div class="card">
    <div class="card-content black-text">
      <div class="row">
        <div class="col s4">
          <span class="card-title black-text">Name</span>
          <p><%= @enquiry.first_name %> <%= @enquiry.last_name %></p>
        </div>
        <div class="col s4">
          <span class="card-title black-text">Phone</span>
          <p><%= @enquiry.phone %></p>
        </div>
        <div class="col s4">
          <span class="card-title black-text">Email</span>
          <p><%= @enquiry.email %></p>
        </div>
      </div><br>
      <div class="row">
        <div class="col s12">
          <span class="card-title black-text">Enquiry</span>
          <p><%= @enquiry.message %></p>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
