<div class="container">
<div class="row">
  <div class="card">
    <div class="card-content black-text">
      <div class="row">
        <div class="col s4">
          <span class="card-title black-text">Name</span>
          <p><%= @expo_attendee.first_name %> <%= @expo_attendee.last_name %></p>
        </div>
        <div class="col s4">
          <span class="card-title black-text">Phone</span>
          <p><%= @expo_attendee.phone %></p>
        </div>
        <div class="col s4">
          <span class="card-title black-text">Email</span>
          <p><%= @expo_attendee.email %></p>
        </div>
      </div><br>
      <div class="row">
        <div class="col s12">
          <span class="card-title black-text">Enquiry Type</span>
          <p><%= @expo_attendee.expo_type %></p>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
